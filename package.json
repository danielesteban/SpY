{
  "name": "SpY",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "build:report": "npm run build --report",
    "deploy": "npm run lint && npm run build && gh-pages -d dist",
    "deploy:travis": "gh-pages -d dist -u \"Travis CI <travis@travis-ci.org>\"",
    "lint": "eslint --cache --ext .js src",
    "lint:fix": "eslint --cache --fix --ext .js src"
  },
  "pre-commit": {
    "run": "lint",
    "silent": true
  },
  "dependencies": {
    "stats.js": "^0.17.0",
    "three": "^0.98.0",
    "touches": "^1.2.2",
    "wheel": "^0.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "2.14.0",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "ghpages-spa-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "robotstxt-webpack-plugin": "^4.0.1",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
